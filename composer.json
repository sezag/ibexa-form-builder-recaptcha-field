{
  "name": "adamwojs/ibexa-form-builder-recaptcha-field",
  "license": "MIT",
  "type": "ibexa-bundle",
  "description": "Bundle providing reCAPTCHA field for the Ibexa Form Builder",
  "repositories": {
    "ibexa": {
      "type": "composer",
      "url": "https://updates.ibexa.co"
    }
  },
  "autoload": {
    "psr-4": {
      "AdamWojs\\IbexaFormBuilderRecaptchaBundle\\": "src/bundle/",
      "AdamWojs\\IbexaFormBuilderRecaptcha\\": "src/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdamWojs\\IbexaFormBuilderRecaptchaBundle\\Tests\\": "src/bundle/Tests"
    }
  },
  "require": {
    "php": ">=8.0",
    "ibexa/form-builder": "^4.0",
    "excelwebzone/recaptcha-bundle": "^1.5",
    "symfony/http-kernel": "^5.0",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/yaml": "^5.0",
    "symfony/form": "^5.0",
    "symfony/options-resolver": "^5.0"
  },
  "require-dev": {
    "ibexa/code-style": "^1.0",
    "friendsofphp/php-cs-fixer": "^3",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  }
}
